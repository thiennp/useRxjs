{
  "name": "use-rxjs",
  "version": "0.0.1",
  "description": "A lightweight, hook-based bridge to manage RxJS observable subscriptions and state synchronization within React components.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./useBehaviorSubject": {
      "types": "./dist/useBehaviorSubject.d.ts",
      "import": "./dist/useBehaviorSubject.mjs",
      "require": "./dist/useBehaviorSubject.js"
    }
  },
  "packageManager": "pnpm@9.15.0",
  "sideEffects": false,
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "pnpm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "rxjs": "^7.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "react": "^18.3.0",
    "rxjs": "^7.8.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiennp/useRxjs.git"
  },
  "keywords": ["react", "rxjs", "hooks", "behavior-subject", "observable"],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thiennp/useRxjs/issues"
  },
  "homepage": "https://github.com/thiennp/useRxjs#readme"
}
